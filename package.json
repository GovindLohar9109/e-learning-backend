{
  "name": "E-Learning-Management-System",
  "version": "1.0.0",
  "description": "This is E-Learning Project which will provide courses to users.",
  "main": "app.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "dev": "nodemon src/app",
    "start": "node src/app.js",
    "seed": "npx prisma db seed",
    "seed-revert-courses": "node src/prisma/seed/seed-revert-courses.js",
    "seed-revert-roles": "node src/prisma/seed/seed-revert-roles.js",
    "seed-revert-users": "node src/prisma/seed/seed-revert-users.js",
    "seed-revert-all": "node src/prisma/seed/seed-revert.js",
    "migrate": "prisma migrate dev --name init",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jest": "^30.2.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.5.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "typescript-eslint": "^8.46.4"
  },
  "prisma": {
    "seed": "node src/prisma/seed/seed.js"
  }
}
